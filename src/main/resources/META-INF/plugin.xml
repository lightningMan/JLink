<idea-plugin>
  <id>org.jetbrains</id>
  <name>JLink</name>
  <description>Generate Links to Intellij code</description>
  <version>0.0.1</version>
  <vendor email="yoazm@jfrog.com" url="yuzlink.com">dummy</vendor>

  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
  </extensions>

  <actions>
    <action id="JLink" class="JLinkGenerator" text="JLink" description="Generates JLinks">
      <add-to-group group-id="EditorPopupMenu" anchor="after" relative-to-action="CopyReference"/>
      <keyboard-shortcut keymap="$default" first-keystroke="meta J"/>
    </action>
  </actions>

</idea-plugin>